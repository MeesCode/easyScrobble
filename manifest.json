{
  "manifest_version": 2,

  "name": "easyScrobble",
  "description": "Scrobble from current last.fm page",
  "version": "1.0",
  "icons": { "32": "icon32.png",
          "128": "icon128.png" },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "cookies"
  ]
}
