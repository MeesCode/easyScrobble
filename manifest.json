{
  "manifest_version": 2,

  "name": "easyScrobble",
  "description": "Scrobble from current last.fm page",
  "version": "1.5.1",
  "icons": {
    "32": "icon32.png",
    "128": "icon128.png"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "cookies",
    "*://www.last.fm/*"
  ]
}
